      "version": ""
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Targets UP",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum(up{job=~\"prometheus|rabbitmq\"})",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 2 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "RabbitMQ Status",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "max(up{job=\"rabbitmq\"})",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "DOWN", "color": "red" },
                "1": { "text": "UP", "color": "green" }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Current Connections",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum(rabbitmq_connections)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Queues",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum(rabbitmq_queues)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Message Flow (global rates/s)",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rate(rabbitmq_global_messages_received_total[1m]))", "legendFormat": "received", "refId": "A" },
        { "expr": "sum(rate(rabbitmq_global_messages_routed_total[1m]))", "legendFormat": "routed", "refId": "B" },
        { "expr": "sum(rate(rabbitmq_global_messages_delivered_total[1m]))", "legendFormat": "delivered", "refId": "C" },
        { "expr": "sum(rate(rabbitmq_global_messages_acknowledged_total[1m]))", "legendFormat": "acked", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Depth (messages)",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rabbitmq_queue_messages)", "legendFormat": "total", "refId": "A" },
        { "expr": "sum(rabbitmq_queue_messages_ready)", "legendFormat": "ready", "refId": "B" },
        { "expr": "sum(rabbitmq_queue_messages_unacked)", "legendFormat": "unacked", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Connections / Channels / Consumers",
      "id": 7,
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rabbitmq_connections)", "legendFormat": "connections", "refId": "A" },
        { "expr": "sum(rabbitmq_channels)", "legendFormat": "channels", "refId": "B" },
        { "expr": "sum(rabbitmq_consumers)", "legendFormat": "consumers", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Connection Churn (opened vs closed /5m)",
      "id": 8,
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rate(rabbitmq_connections_opened_total[5m]))", "legendFormat": "opened", "refId": "A" },
        { "expr": "sum(rate(rabbitmq_connections_closed_total[5m]))", "legendFormat": "closed", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "gauge",
      "title": "Erlang Processes Used (%)",
      "id": 9,
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "100 * max(rabbitmq_erlang_processes_used) / max(rabbitmq_erlang_processes_limit)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "orange", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Memory & Disk",
      "id": 10,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "rabbitmq_process_resident_memory_bytes", "legendFormat": "resident_memory", "refId": "A" },
        { "expr": "rabbitmq_resident_memory_limit_bytes", "legendFormat": "memory_limit", "refId": "B" },
        { "expr": "rabbitmq_disk_space_available_bytes", "legendFormat": "disk_available", "refId": "C" },
        { "expr": "rabbitmq_disk_space_available_limit_bytes", "legendFormat": "disk_limit", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "I/O Throughput (bytes/s)",
      "id": 11,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rate(rabbitmq_io_read_bytes_total[1m]))", "legendFormat": "read", "refId": "A" },
        { "expr": "sum(rate(rabbitmq_io_write_bytes_total[1m]))", "legendFormat": "write", "refId": "B" },
        { "expr": "sum(rate(rabbitmq_connection_incoming_bytes_total[1m]))", "legendFormat": "conn_in", "refId": "C" },
        { "expr": "sum(rate(rabbitmq_connection_outgoing_bytes_total[1m]))", "legendFormat": "conn_out", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Auth Attempts (rate/s)",
      "id": 12,
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rate(rabbitmq_auth_attempts_total[1m]))", "legendFormat": "total", "refId": "A" },
        { "expr": "sum(rate(rabbitmq_auth_attempts_succeeded_total[1m]))", "legendFormat": "success", "refId": "B" },
        { "expr": "sum(rate(rabbitmq_auth_attempts_failed_total[1m]))", "legendFormat": "failed", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Backend RabbitMQ Queue Activity",
      "id": 13,
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        { "expr": "sum(rate(rabbitmq_queue_messages_published_total{queue=~\"orders.*\"}[1m]))", "legendFormat": "orders_published", "refId": "A" },
        { "expr": "sum(rabbitmq_queue_messages_ready{queue=~\"orders.*\"})", "legendFormat": "orders_ready", "refId": "B" },
        { "expr": "sum(rabbitmq_queue_messages_unacked{queue=~\"orders.*\"})", "legendFormat": "orders_unacked", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    }
  ],
